{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" },
    { "src": "public/**/*.pug", "use": "@vercel/static" },
    { "src": "static/**/*", "use": "@vercel/static" }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/app.js"
    }
  ],
  "cleanUrls": true,
  "framework": "null",
  "installCommand": "npm install"
}
