{
  "version": 2,
  "builds": [
    { "src": "route/index.js", "use": "@vercel/node" },
    { "src": "public/**/*.pug", "use": "@vercel/static" },
    { "src": "static/**/*", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/route/index.js" }
  ],
  "buildCommand": "node app.js",
  "cleanUrls": true,
  "installCommand": "npm install"
}

